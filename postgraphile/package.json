{
  "name": "@playground/postgraphile",
  "version": "0.1.0",
  "private": true,
  "description": "Comparing few graphql server/db solutions",
  "keywords": [
    "dgraph",
    "faunadb",
    "ormless",
    "postgraphile",
    "prisma",
    "typeorm"
  ],
  "homepage": "https://vadistic.netlify.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/vadistic/graphql-db-playground"
  },
  "license": "MIT",
  "author": "Jakub Wadas <vadistic@gmail.com>",
  "scripts": {
    "build": "tsc -b -v tsconfig.build.json",
    "clean": "rm -rf dist",
    "dev": "ts-node server/main.ts",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "lint": "eslint '*/**/*.{ts,tsx}'",
    "lint:fix": "eslint '*/**/*.{ts,tsx}' --fix",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:watch": "jest --watch",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@graphile-contrib/pg-many-to-many": "1.0.0-beta.12",
    "@graphile-contrib/pg-simplify-inflector": "5.0.0-beta.1",
    "apollo-server-express": "^2.9.7",
    "dotenv": "8.2.0",
    "express": "4.17.3",
    "graphql": "^14.5.8",
    "postgraphile": "4.4.4"
  },
  "devDependencies": {
    "@types/dotenv": "6.1.1",
    "@types/express": "4.17.1",
    "@types/faker": "4.1.6",
    "@types/node": "12.11.1",
    "@types/pg": "7.11.2",
    "faker": "4.1.0",
    "knex": "0.19.5",
    "pg": "7.12.1"
  },
  "engines": {
    "node": ">= 10.x"
  }
}
