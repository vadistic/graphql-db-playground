{
  "name": "@playground/dgraph",
  "version": "0.1.0",
  "private": true,
  "description": "Comparing few graphql server/db solutions",
  "keywords": [
    "dgraph",
    "faunadb",
    "ormless",
    "postgraphile",
    "prisma",
    "typeorm"
  ],
  "homepage": "https://vadistic.netlify.com",
  "repository": "",
  "license": "MIT",
  "author": "Jakub Wadas <vadistic@gmail.com>",
  "scripts": {
    "build": "tsc -b -v tsconfig.build.json",
    "clean": "rm -rf dist",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "lint": "eslint '*/**/*.{ts,tsx}'",
    "lint:fix": "eslint '*/**/*.{ts,tsx}' --fix",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:watch": "jest --watch",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "apollo-server-express": "2.9.7",
    "dgraph-js": "2.0.2",
    "dotenv": "8.2.0",
    "express": "4.17.3",
    "graphql": "14.5.8",
    "grpc": "1.24.0"
  },
  "devDependencies": {
    "@types/dotenv": "6.1.1",
    "@types/express": "4.17.1",
    "@types/faker": "4.1.6",
    "@types/jest": "24.0.19",
    "@types/node": "12.11.5",
    "@vadistic/eslint-config-node": "0.1.5",
    "@vadistic/prettier-config": "0.1.4",
    "eslint": "6.5.1",
    "faker": "4.1.0",
    "jest": "24.9.0",
    "prettier": "1.18.2",
    "ts-jest": "24.1.0",
    "ts-node": "8.4.1",
    "tsc-watch": "4.0.0",
    "typescript": "3.7.1-rc"
  },
  "engines": {
    "node": ">= 10.x"
  }
}
