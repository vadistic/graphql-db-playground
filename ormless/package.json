{
  "name": "@playground/ormless",
  "version": "0.1.7",
  "private": true,
  "description": "Comparing few graphql server/db solutions",
  "keywords": [
    "dgraph",
    "faunadb",
    "ormless",
    "postgraphile",
    "prisma",
    "typeorm"
  ],
  "homepage": "https://vadistic.netlify.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/vadistic/graphql-db-playground"
  },
  "license": "MIT",
  "author": "Jakub Wadas <vadistic@gmail.com>",
  "module": "dist/main.js",
  "source": "scr/main.ts",
  "scripts": {
    "build": "tsc -b -v tsconfig.build.json",
    "clean": "rm -rf dist",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "lint": "eslint '*/**/*.{ts,tsx}'",
    "lint:fix": "eslint '*/**/*.{ts,tsx}' --fix",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:watch": "jest --watch",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@types/pg": "^7.11.2"
  },
  "engines": {
    "node": ">= 10.x"
  },
  "dependencies": {
    "pg": "^7.12.1"
  }
}
